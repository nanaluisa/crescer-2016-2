@model StreetFighter.Web.Models.SobreModel
@{
    ViewBag.Title = "Sobre";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="corpo">
    <h1 id="sobre">Anna Silva</h1>
    <table>
        <tr>
            <td>
                <img id="imagem" src="~/Content/Images/girl-in-tech.jpg" alt="Imagem-Girl-In-Tech" />
            </td>
            <td>
                <p>
                    <strong>Nome Completo:</strong> @Model.NomeCompleto</br>
                    <strong>Nascimento:</strong> @Model.DataNascimento</br>
                    <strong>Altura:</strong>@Model.Altura cm. </br>
                    <strong>Filiação:</strong> @Model.Filiacao </br>
                    <strong>País de Origem:</strong> @Model.PaisDeOrigem  </br>
                    <strong>Cidade Natal:</strong> @Model.CidadeNatal </br>
                    <strong>Área de Formação:</strong>@Model.AreaDeFormacao </br>
                    <strong>Universidade:</strong>@Model.Universidade </br>
                    <strong>Habilidades especiais: </strong>@Model.Habilidades </br>
                    <strong>Gosta:</strong>@Model.Gosta </br>
                    <strong>Desgosta:</strong> @Model.Desgosta </br>
                    <strong>Bebida Predileta:</strong>@Model.BebidaPredileta </br>
                    <strong>Frase:</strong>@Model.FalaDeVitoria </br>
                    <strong>Uma fala de vitória:</strong>@Model.FalaDeVitoria </br>
                    <strong>Nickname:</strong> @Model.Nickname </br>
                    <strong>Mentor:</strong> @Model.Mentor</br>
                    <strong>Hobbies:</strong> @Model.Hobbies </br>
                    <strong>Bandas Favoritas:</strong>@Model.BandasFavoritas</br>
                    <strong>Objetivo desta Page:</strong>@Model.ObjetivoDaPage </br>
                </p>
            </td>
        </tr>

    </table>
</div>














